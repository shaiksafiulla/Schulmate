@model SchoolCoreWEB.Models.VRole
@using Localization.Services
@inject LanguageService _localization

    <div class="card">
        <div class="card-body">
           <div class="row">
                <div class="col-5">
                    <label>@_localization.Getkey("Name") : </label>
                </div>
                <div class="col-7">
                    @Html.DisplayFor(model => model.Name)
                </div>
            </div>
             <div class="row">
                <div class="col-5">
                <label>@_localization.Getkey("Description") : </label>
                </div>
                <div class="col-7">
                    @Html.DisplayFor(model => model.Description)
                </div>
            </div>
             <div class="row">
                <div class="col-5">
                <label>@_localization.Getkey("NoOfUsers") : </label>
                </div>
                <div class="col-7">
                    @Html.DisplayFor(model => model.UserCount)
                </div>
            </div>
              <div class="row">
                <div class="col-5">
                <label>@_localization.Getkey("Branch") : </label>
                </div>
                <div class="col-7">
                    @Html.DisplayFor(model => model.BranchName)
                </div>
            </div>
              <div class="row">
                <div class="col-5">
                <label>@_localization.Getkey("SessionYear") : </label>
                </div>
                <div class="col-7">
                    @Html.DisplayFor(model => model.SessionYearName)
                </div>
            </div>
        </div>
    </div>   

